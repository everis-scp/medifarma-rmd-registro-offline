<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
    <Dialog class="sapUiContentPadding sapUiSizeCompact" afterClose="onCancelVisualizeEtiquetaMuestreo" title="{i18n>tlVisualizeEtiqueta}" contentWidth="470px">
        <content>
            <Carousel width="450px" height="400px" id="carouselDataMuestreo" loop="true"/>
        </content>
        <beginButton>
            <Button text="{i18n>mnPrint}" press="onConfirmPrintEtiquetaMuestreo" type="Accept"/>
        </beginButton>
        <endButton>
            <Button text="{i18n>btnClose}" press="onCancelVisualizeEtiquetaMuestreo" type="Reject"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>