<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="Adicionar Equipos: EQUIPOS" afterClose="onCancelAddEquipoEditRM">
        <core:Fragment fragmentName="mif.rmd.registro.view.fragment.editarRM.filter.FilterAdEquipos" type="XML"/>
        <core:Fragment fragmentName="mif.rmd.registro.view.fragment.editarRM.body.BodyAdEquipos" type="XML"/>
        <beginButton>
            <Button text="{i18n>btnAgregar}" press="onConfirmAddEquipment" type="Emphasized"/>
        </beginButton>
        <endButton>
			<Button text="{i18n>btnClose}" press="onCancelAddEquipoEditRM" type="Transparent"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>