<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:dnd="sap.ui.core.dnd">
	<!-- <Dialog title="{asociarDatos>/codigo} - {asociarDatos>/descripcion}" afterClose="onCancelCuadroResponsive"> -->
		<ScrollContainer
		height="100%"
		width="100%"
		horizontal="false"
		vertical="true">	
		<Table width="auto" sticky="HeaderToolbar,ColumnHeaders" mode="None" items="{aListInsumosAssignResponsive>/}" updateFinished="onUpdateFinish" growing="true"
			growingScrollToLoad="true">
			<headerToolbar>
				<OverflowToolbar>
					<!-- <Title text="{i18n>tltEquipos} ({=${aListEquipoAssignResponsive>/}.length})"></Title> -->
					<ToolbarSpacer/>
					<ToolbarSeparator/>
					<Button icon="sap-icon://multiselect-all" tooltip="{i18n>btnListInsumos}" press="onListInsumosCP" type="Emphasized" enabled="{aListInsumosAssignResponsive>/0/generalEnabledSaveButton}"/>
				</OverflowToolbar>
			</headerToolbar>
			<columns>
				<Column width="20%" hAlign="Left" vAlign="Middle" styleClass="cellBorderLeft cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>clDescription}" width="auto"/>
					<footer>
						<Text text="{i18n>ftTVerificado}{aFooter>/usuario}" visible="{estructuraVerificado>/state}"/>
					</footer>
				</Column>
				<Column width="10%" hAlign="Left" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>clCodigo}" width="auto"/>
				</Column>
				<Column width="8%" hAlign="Center" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>clAdicional}" width="auto"/>
				</Column>
				<Column width="5%" hAlign="Center" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>clUM}" width="auto"/>
				</Column>
				<Column width="10%" hAlign="End" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>clCount}" width="auto"/>
                    <footer>
						<Text text="{i18n>ftTFecha}{aFooter>/fecha}" visible="{estructuraVerificado>/state}"/>
					</footer>
				</Column>
				<Column width="12%" hAlign="End" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>clCantidadOP}" width="auto"/>
				</Column>
                <Column width="15%" hAlign="End" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>clCountGive}" width="auto"/>
				</Column>
                <Column width="10%" hAlign="Center" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>clBulto}" width="auto"/>
				</Column>
                <Column width="10%" hAlign="Center" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="" width="auto"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<Text text="{aListInsumosAssignResponsive>Maktx}" width="auto"/>
                    <Text text="{aListInsumosAssignResponsive>Component}" width="auto"/>
					<Text text="N" width="auto"/>
					<Text text="{aListInsumosAssignResponsive>CompUnit}" width="auto"/>
                    <Text text="{aListInsumosAssignResponsive>CompQty}" width="auto"/>
					<Text text="{aListInsumosAssignResponsive>CompQty}" width="auto"/>
                    <Text text="{aListInsumosAssignResponsive>cantidadBarCode}" width="auto"/>
					<Text text="{aListInsumosAssignResponsive>numeroBultos}" width="auto"/>
					<VBox>
						<CheckBox selected="{aListInsumosAssignResponsive>verifCheck}" enabled="false"/>
						<ObjectStatus text="{aListInsumosAssignResponsive>usuarioActualiza}" visible="{aListInsumosAssignResponsive>generalVisibleTextUsuario}" state="{path: 'aListInsumosAssignResponsive>styleUser',formatter: '.formatter.onGetUserStyles'}"/>
						<!-- <ObjectStatus text="{parts: [{path: 'aListInsumosAssignResponsive>aplica'}, {path: 'aListInsumosAssignResponsive>flagEditado'}], formatter: '.formatter.onGetModifOrNoApply'}" visible="{aListInsumosAssignResponsive>generalVisibleTextAdic}" state="{parts: [{path: 'aListInsumosAssignResponsive>aplica'}, {path: 'aListInsumosAssignResponsive>flagEditado'}], formatter: '.formatter.onGetModifOrNoApplyStyles'}"/> -->
					</VBox>
				</ColumnListItem>
			</items>
		</Table>
	</ScrollContainer>
		<!-- <endButton>
			<Button text="{i18n>btnClose}" press="onCancelCuadroResponsive" type="Transparent"/>
		</endButton>
	</Dialog> -->
	</core:FragmentDefinition>